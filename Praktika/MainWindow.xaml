<Window x:Class="Praktika.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Praktika"
        mc:Ignorable="d"
         Title="MainWindow" Height="550" Width="800">
    <Grid Background="#FFD7F5F0">
        <Grid.RowDefinitions>
            <RowDefinition Height="37*"/>
            <RowDefinition Height="180*"/>
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Height="40" x:Name="BackButt" Click="BackButt_Click">Назад</Button>
        <Frame NavigationUIVisibility="Hidden" x:Name="MainFrame" Grid.Row="1"/>
    </Grid>
</Window>
