<Page x:Class="Praktika.Pages.NavigationPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Praktika.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="NavigationPage">

    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Button x:Name="StudButt" Click="StudButt_Click">Список студентов</Button>
            <Button x:Name="EmpButt" Click="EmpButt_Click">Список сотрудников</Button>
            <Button x:Name="DiscButt" Click="DiscButt_Click">Список дисциплин</Button>
            <Button x:Name="ExamButt" Click="ExamButt_Click">Список экзаменов</Button>
        </StackPanel>
    </Grid>
</Page>
